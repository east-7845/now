<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.now.dao.ILoginDao">

	<select id="loginCheck" parameterType="com.now.vo.EmployeeVO" resultType="com.now.vo.EmployeeVO">
		SELECT EMP_NO
			 , EMP_PASS
		 FROM EMPLOYEE
		WHERE EMP_NO = #{emp_no}
		  AND EMP_PASS = #{emp_pass}
	</select>

</mapper>